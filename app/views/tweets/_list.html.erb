<div class="column">
	<nav class="panel">
		<p class="panel-heading">Whom to Follow</p>
	<% @users.each do | user | %>
	<div class="panel-block">
		<article class="media">
			<div class="media-content">
				<div class="content">
					<p>
						<%= link_to user_path(user) do %>
						<strong><%= user.name %></strong>
						<small>@<%= user.user_name %></small>
					<% end %>
					</p>
				</div>
			</div>
		</article>
	</div>
    <% end %>
	</nav>
</div>
